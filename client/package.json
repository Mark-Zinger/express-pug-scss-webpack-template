{
  "name": "express-pug-scss-webpack-template",
  "version": "1.0.0",
  "description": "Basic configuration of Express Pug SCSS Webpack",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --open",
    "dev": "cross-env NODE_ENV=development webpack",
    "build": "cross-env NODE_ENV=production webpack",
    "component:create": "node scripts/component/create.js && npm run component:refresh",
    "component:remove": "node scripts/component/remove.js && npm run component:refresh",
    "component:refresh": "node scripts/component/refresh.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mark-Zinger/express-pug-scss-webpack-template.git"
  },
